{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "format:check": "prettier --ignore-path .gitignore --check .",
    "format:fix": "prettier --ignore-path .gitignore --write .",
    "lint:check": "eslint --ignore-path .gitignore --ext .jsx,.js,.tsx,.ts ./",
    "lint:fix": "npm run lint:check -- --fix",
    "lint:tsc": "tsc --noEmit --project tsconfig.json",
    "prepare": "husky install",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch --verbose",
    "start": "NODE_ENV=production node ./dist/index.js",
    "dev": "NODE_ENV=development nodemon",
    "build": "tsc && tsc-alias",
    "seed:pre": "rm -rf /.dist && npm run build",
    "seed": "npm run seed:pre && knex seed:run",
    "migrate:prod": "NODE_ENV=production knex migrate:latest",
    "migrate:dev": "NODE_ENV=development knex migrate:latest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "express-rate-limit": "6.7.0",
    "helmet": "6.0.1",
    "knex": "2.4.2",
    "morgan": "1.10.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/morgan": "^1.9.4",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  }
}
